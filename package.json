{
    "name": "ts-project",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon ./src/index.ts",
        "test": "cross-env NODE_ENV=test jest",
        "start": "cross-env NODE_ENV=production node dist/index.js",
        "build": "tsc -p tsconfig.build.json",
        "commit": "commit",
        "format": "prettier ./src/**/*.ts --write",
        "lint": "eslint ./src/**/*.ts --fix",
        "prepare": "husky install",
        "migrate:dev": "dotenv  -e .env.development npx prisma migrate dev",
        "push:dev": "dotenv  -e .env.development npx prisma db push",
        "migrate:test": "dotenv  -e .env.test npx prisma migrate dev",
        "push:test": "dotenv  -e .env.test npx prisma db push",
        "migrate:prod": "dotenv  -e .env.production npx prisma migrate dev",
        "push:prod": "dotenv  -e .env.production npx prisma db push",
        "deploy": "dotenv -e .env.production npx prisma migrate deploy"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^5.5.0",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.7.2",
        "@commitlint/config-conventional": "^17.7.0",
        "@commitlint/prompt-cli": "^17.7.2",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.15",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.5",
        "@types/node": "^20.6.0",
        "@types/supertest": "^2.0.15",
        "@typescript-eslint/eslint-plugin": "^6.7.3",
        "@typescript-eslint/parser": "^6.7.3",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^7.3.0",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "husky": "^8.0.0",
        "jest": "^29.7.0",
        "lint-staged": "^14.0.1",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.3",
        "prisma": "^5.5.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    }
}
